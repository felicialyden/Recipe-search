generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
}

model Saved {
  id       Int    @id @default(autoincrement())
  recipeId Int
  title    String
  image    String
  userId   String
}
